<!-- get parameter from launch command, joinst stage for wheel, spin onceros2 launch attach_shelf pre_approach.launch.xml obstacle:=0.3 degrees:=-90 -->

<launch>

    <arg name="obstacle" default="0.8"/>
    <arg name="degrees" default="0.5"/>

    <node pkg="attach_shelf" exec="pre_approach_node" name="pre_approach">
        <param name="obstacle" value="$(var obstacle)"/>
        <param name="degrees" value="$(var degrees)"/>
    </node>

</launch>


